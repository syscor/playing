<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]> <html class="no-js"><![endif]-->

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>SysCor S.A. Monitoring System</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />

        <!--Bootstrap-->
        <link href="~/Content/bootstrap/bootstrap.min.css" rel="stylesheet"/>
        <link href="~/Content/bootstrap/bootstrap-responsive.min.css" rel="stylesheet"/>
        <!--Wijmo Theme-->
        <link href="~/Content/themes/rocket/jquery-wijmo.css" rel="stylesheet" type="text/css" title="rocket-jqueryui"/>
        <link href="~/Content/jquery.wijmo-complete.2.3.5.css" rel="stylesheet" type="text/css" />

        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <header>
            <div class="navbar navbar-inverse navbar-fixed-top">
                <div class="navbar-inner">
                    <div class="container-fluid">
                        <div class="logo">
                            <a href="#">
                                <img src="~/Content/images/genericuse/logo1.gif" alt="SysCor"/>
                            </a>
                        </div>
                        <ul class="nav pull-right">
                            <li class="dropdown">
                                <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown"><img src="~/Content/images/genericuse/settings.png" alt="Configuración" width="18"/> <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li class="nav-header">Hardware</li>
                                    <li><a href="javascript:void(0);" onclick="SysCor.performViewChange('/Config/BoardSettings');">Tarjetas DAQ</a></li>
                                    <li><a href="javascript:void(0);" onclick="SysCor.performViewChange('/Sensor/SensorPanel');">Sensores</a></li>
                                    <li class="divider"></li>
                                    <li><a href="javascript:void(0);" onclick="SysCor.performViewChange('/Config/RestoreSettings');">Inicializar Configuraciones</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
       @*Loading Image*@
        <div id="fountainG" style="position: fixed; z-index: 999999; top: 35%; left: 35%">
            <div id="fountainG_1" class="fountainG">
            </div>
            <div id="fountainG_2" class="fountainG">
            </div>
            <div id="fountainG_3" class="fountainG">
            </div>
            <div id="fountainG_4" class="fountainG">
            </div>
            <div id="fountainG_5" class="fountainG">
            </div>
            <div id="fountainG_6" class="fountainG">
            </div>
            <div id="fountainG_7" class="fountainG">
            </div>
            <div id="fountainG_8" class="fountainG">
            </div>
        </div>
        @*Container for alerts*@
        <div id="alertsContainer" style="position: fixed; right: 15%; z-index: 9999; max-width: 500px;">
        </div>
       
        <div id="bodyContainer" class="container" style="margin-top: -15px;">
            @RenderSection("featured", false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer class="navbar-fixed-bottom">
            <div class="container">
                <p class="pull-right">
                    &copy; @DateTime.Now.Year - SysCor Systems | <a target="_blank" href="mailto:info@syscor.com?Subject=SysControl v1 Info Request">info@syscor.com</a> | 4431 725600 &nbsp;&nbsp;&nbsp;&nbsp; 
                    <a href="http://www.linkedin.com/company/288446?trk=tyah" target="_blank"><img width="21" height="20" alt="LinkedIn" src="/Content/images/genericuse/linkedIn.png"/></a>                
                    <a href="https://www.facebook.com/xxxx" target="_blank"><img width="21" height="20" alt="facebook" src="/Content/images/genericuse/facebook.png"/></a> 
                    <a href="https://twitter.com/#!/xxx" target="_blank"><img width="21" height="20" alt="twitter" src="/Content/images/genericuse/twiiter.png"/></a> 
                </p>

            </div>  
        </footer>
            <div class="clearfix"></div>
       
        

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/Content/js")
        @Scripts.Render("~/bundles/jqueryui")
        <script src="~/Content/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
        <script src="~/Scripts/knockout-2.2.0.js" type="text/javascript"></script>
        <script src="~/Scripts/knockout.mapping-latest.js" type="text/javascript"></script>
        <script src="~/Scripts/knockout.validation.js" type="text/javascript"></script>
        <script src="~/Scripts/raphael-min.js" type="text/javascript"></script>
        <script src="~/Scripts/justgage.1.0.1.min.js" type="text/javascript"></script>
        <!--Wijmo Widgets JavaScript-->
        <script src="~/Scripts/jquery.wijmo-open.all.2.3.4.min.js" type="text/javascript"></script>
        <script src="~/Scripts/jquery.wijmo-complete.all.2.3.4.min.js" type="text/javascript"></script>
        <script src="~/Content/Dynatree/jquery.dynatree.min.js"></script>

        <script type="text/javascript">
            SysCor.initialize();
        </script>
        @RenderSection("scripts", false)
    </body>
</html>